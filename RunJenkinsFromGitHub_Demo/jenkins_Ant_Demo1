pipeline {
	agent any

	options {
  	skipDefaultCheckout true
	}

	stages {
		stage("Checkout build-tool-overkill") {
			steps {
				
				echo "Running ANT build..."
				bat "ant -version"

				echo "Checking out build-tool-overkill..."

				checkout changelog: false,
				poll: false,
				scm: [$class: 'GitSCM',
					branches: [[name: '*/master']],
					extensions: [],
					userRemoteConfigs: [[url: 'https://github.com/hannotify/build-tool-overkill']]]
			}
		}

		stage("Build") {
			steps {
				echo "Build build-tool-overkill with Ant..."
				
			}
		}
	}
}
